digraph Program {
    // Styling
    node [shape=box, style="rounded"];  // Rounded boxes

    // Nodes
    main [label="Main"];
    ast [label="AST"];
    lexer [label="Lexer"];
    parser [label="Parser"];
    prepro [label="Preprocessor"];
    writer [label="Writer"];
    reporting [label="Reporting"];
    token [label="Token"];

    // Edges (connections)

    lexer -> main;
    parser -> main;
    ast -> main;
    prepro -> main;
    writer -> main;

    reporting -> lexer;
    

    parser -> prepro;

    parser -> writer;
    prepro -> writer;
}